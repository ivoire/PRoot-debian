#!/usr/bin/make -f
# -*- makefile -*-
%:
	dh $@ --sourcedirectory=src

# Force make to print the commands
# By default V=0
override_dh_auto_build:
	make -C doc proot.1
	dh_auto_build -- V=1

override_dh_auto_install:
	dh_auto_install -- PREFIX=/usr

override_dh_auto_clean:
	rm -f doc/proot.1
	dh_auto_clean
